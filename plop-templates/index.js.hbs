import * as React from 'react';
import { PageLayout, Text } from '@tarantool.io/ui-kit';

const projectName = '{{name}}';

const Root = () => (
  <PageLayout heading={projectName}>
    <Text>{`${projectName} content`}</Text>
  </PageLayout>
);

window.tarantool_enterprise_core.register(
  projectName,
  [
    {
      label: '{{name}}',
      path: '{{name}}',
    }
  ],
  Root,
  'react'
);

window.tarantool_enterprise_core.install();
